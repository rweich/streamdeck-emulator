{
  "name": "@rweich/streamdeck-emulator",
  "version": "0.0.0-development",
  "description": "Emulates a streamdeck to speed up plugin development.",
  "author": "rweich",
  "license": "MIT",
  "private": "true",
  "scripts": {
    "build": "webpack --mode=development",
    "build-prod": "webpack --mode=production",
    "lint": "eslint ./src/**/*.ts",
    "lint-tests": "eslint test/**/*.ts",
    "prepare": "husky install",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@rweich/streamdeck-events": "^1.1.0",
    "browser-sync": "^2.26.14",
    "chokidar": "^3.5.1",
    "eventemitter3": "^4.0.7",
    "isomorphic-ws": "^4.0.1",
    "jsdom": "^16.5.3",
    "loglevel": "^1.7.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "ts-log": "^2.2.3",
    "ts-type-guards": "^0.7.0",
    "when-dom-ready": "^1.2.12",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rweich/eslint-config": "^1.1.0",
    "@rweich/prettier-config": "^1.0.0",
    "@types/browser-sync": "^2.26.1",
    "@types/jsdom": "^16.2.10",
    "@types/ws": "^7.4.4",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "esm": "^3.2.25",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^2.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": "rweich/streamdeck-emulator",
  "bugs": "https://github.com/rweich/streamdeck-emulator/issues",
  "keywords": [
    "streamdeck",
    "elgato",
    "typescript",
    "emulator"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": "@rweich"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "eslint --quiet --fix"
    ]
  },
  "prettier": "@rweich/prettier-config"
}
